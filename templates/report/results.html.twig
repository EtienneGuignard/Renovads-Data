
{% extends '@EasyAdmin/page/content.html.twig' %}
{% block content_title %}Report Result{% endblock %}

{% block main %}
<div class="row">
{% include "report/form.html.twig" %}
<div class="row mt-5">
{% for global in resultGlobal %}
    <p><span class="labelSelectForm">Number of selected lead: </span> {{global.NumberOfLeads}}</p>
{% endfor %}

</div>
<div class="row mt-3">
<div class="col col-lg-6">
    {% include "report/chartReport.html.twig" %}
</div>
<div class="col col-lg-6">
    <table class="datagrid">
        <thead>
            <tr>
                <td>timestamp</td>
                <td>email</td>
                <td>Supplier</td>
                <td>status</td>
            </tr>
        </thead>
        <tbody>
        {% for result in results %}
              <tr>
                    <td>{{result.created_at}}</td>
                    <td>{{result.email}}</td>
                    <td>{{result.reference}}</td>
                    <td>{{result.status}}</td>
                </tr>
        {% endfor %}

              

        </tbody>
    </table>
        </div>
    </div>
</div>
{% endblock %}
 {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
    {% endblock %}